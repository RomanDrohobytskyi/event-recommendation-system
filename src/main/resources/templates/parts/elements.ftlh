<#macro input id="" name="" value="" type="" placeholder="" onfocus="" onblur="" onclick="" class=""
style="text-align:center; width: 250px;" step="" min="" max="">
    <input id="${id}" name="${name}" value="${value}" type="${type}" placeholder="${placeholder}" onfocus="${onfocus}"
           onblur="${onblur}" class="${class}" style="${style}" step="${step}" min="${min}" max="${max}"/>
</#macro>


